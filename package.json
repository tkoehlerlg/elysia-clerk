{
  "name": "elysia-clerk",
  "version": "0.3.1",
  "description": "Unofficial Clerk plugin for Elysia.",
  "author": "Robert Soriano <sorianorobertc@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/wobsoriano/elysia-clerk#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/elysia-clerk.git"
  },
  "bugs": "https://github.com/wobsoriano/elysia-clerk/issues",
  "keywords": [
    "bun",
    "auth"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "bun run --watch ./dev/index.tsx",
    "build": "bunchee",
    "prepublishOnly": "bun run build",
    "release": "bumpp && npm publish",
    "lint": "biome check src tests",
    "format": "biome format src tests --write",
    "test": "bun test",
    "test:watch": "bun test --watch"
  },
  "peerDependencies": {
    "elysia": "^1.0.0"
  },
  "dependencies": {
    "@clerk/backend": "^0.38.1",
    "@elysiajs/eden": "^1.0.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@elysiajs/html": "^1.0.2",
    "@types/bun": "^1.0.5",
    "bumpp": "^9.3.0",
    "bunchee": "^4.4.6",
    "elysia": "^1.0.9",
    "typescript": "5.3.3"
  }
}
